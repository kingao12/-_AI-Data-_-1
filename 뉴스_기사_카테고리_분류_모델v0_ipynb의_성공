      "cell_type": "code",
      "source": [
        "!pip install konlpy\n",
        "from konlpy.tag import Okt\n",
        "from tqdm import tqdm\n",
        "\n",
        "okt = Okt()\n",
        "poss_dic = {}\n",
        "\n",
        "# Check if art_df is empty and handle it\n",
        "if art_df.empty:\n",
        "    print(\"art_df is empty. Please check your data.\")\n",
        "else:\n",
        "    for i in tqdm(art_df.index):\n",
        "        main = art_df.loc[i, \"main\"]\n",
        "        poss = okt.pos(main, norm=True, stem=True)\n",
        "\n",
        "        poss_lst = []\n",
        "        for word, tag in poss:\n",
        "            if tag in ['Noun', 'Adjective', 'Verb', 'Adverb']:\n",
        "                poss_lst.append(word)\n",
        "\n",
        "        poss_dic[i] = poss_lst\n",
        "\n",
        "    # Try accessing a key that likely exists\n",
        "    if not poss_dic:\n",
        "        print(\"poss_dic is empty. Check the contents of art_df and the loop logic.\")\n",
        "    else:\n",
        "        first_key = list(poss_dic.keys())[0]  # Get the first available key\n",
        "        print(poss_dic[first_key])  # Access using the first available key"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },

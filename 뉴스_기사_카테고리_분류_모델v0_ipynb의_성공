      "cell_type": "code",
      "source": [
        "# 평가\n",
        "print(\"훈련 세트 정확도: {:.3f}\".format(model.score(x_train, y_train)) )\n",
        "print(\"테스트 세트 정확도: {:.3f}\".format(model.score(x_test, y_test)) )"
      ],
